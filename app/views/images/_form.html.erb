

<div class="form-group">
<%= form_for @image, html: { multipart: true }, builder: CustomFormBuilder do |f|  %>
  <%= f.form_error %>


  <%=f.label :description %>
  <%=f.text_field :description, class: 'form-control' %>
    <%= f.field_error :description %>
    <br>
  <%=f.label :imagen %><br>
  <%= f.file_field :picture %>
  <%= f.submit 'Enviar', class: 'btn btn-primary mt-3' %>
<%end  %>
</div>
